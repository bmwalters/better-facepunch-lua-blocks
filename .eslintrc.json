{
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "globals": {
    "CodeMirror": true
  },
  "env": {
    "browser": true,
    "webextensions": true,
    "es6": true
  },
  "rules": {
    "accessor-pairs": 1,
    "array-bracket-spacing": 1,
    "array-callback-return": 2,
    "arrow-body-style": 0,
    "arrow-parens": 1,
    "arrow-spacing": 1,
    "block-scoped-var": 1,
    "block-spacing": [1, "always"],
    "brace-style": [1, "1tbs", { "allowSingleLine": true }],
    "callback-return": 1,
    "camelcase": [1, { "properties": "never" }],
    "class-methods-use-this": 0,
    "comma-dangle": 0,
    "comma-spacing": 1,
    "comma-style": [1, "last"],
    "complexity": 1,
    "computed-property-spacing": 0,
    "consistent-return": 0,
    "consistent-this": [1, "self"],
    "constructor-super": 2,
    "curly": [1, "all"],
    "default-case": 0,
    "dot-location": 0,
    "dot-notation": 0,
    "eol-last": 0, // handled by editor
    "eqeqeq": 0,
    "func-call-spacing": [1, "never"],
    "func-names": 0,
    "func-style": 0,
    "generator-star-spacing": 0,
    "global-require": 1,
    "guard-for-in": 1,
    "handle-callback-err": 1,
    "id-blacklist": 0,
    "id-length": 0,
    "id-match": 0,
    "indent": 0, // handled by editor, see .editorconfig
    "init-declarations": 0,
    "jsx-quotes": 0,
    "key-spacing": 1,
    "keyword-spacing": [1, { "before": true, "after": true }],
    "line-comment-position": 0,
    "linebreak-style": 0, // handled by editor
    "lines-around-comment": 0,
    "lines-around-directive": 0,
    "max-depth": [1, { "max": 5 }],
    "max-len": 0,
    "max-lines": 0,
    "max-nested-callbacks": [1, { "max": 4 }],
    "max-params": 0,
    "max-statements": 0,
    "max-statements-per-line": 0,
    "multiline-ternary": 0,
    "new-cap": 0,
    "new-parens": 1,
    "newline-after-var": 0,
    "newline-before-return": 0,
    "newline-per-chained-call": 0,
    "no-alert": 1,
    "no-array-constructor": 1,
    "no-bitwise": 0,
    "no-caller": 2,
    "no-case-declarations": 1,
    "no-catch-shadow": 1,
    "no-class-assign": 1,
    "no-cond-assign": 2,
    "no-confusing-arrow": 1,
    "no-console": 0,
    "no-const-assign": 2,
    "no-constant-condition": 1,
    "no-continue": 0,
    "no-control-regex": 0,
    "no-debugger": 1,
    "no-delete-var": 2,
    "no-div-regex": 0,
    "no-dupe-args": 2,
    "no-dupe-class-members": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-duplicate-imports": 1,
    "no-else-return": 0,
    "no-empty": 1,
    "no-empty-character-class": 1,
    "no-empty-function": 1,
    "no-empty-pattern": 1,
    "no-eq-null": 0,
    "no-eval": 2,
    "no-ex-assign": 1,
    "no-extend-native": 2,
    "no-extra-bind": 1,
    "no-extra-boolean-cast": 1,
    "no-extra-label": 1,
    "no-extra-parens": 0,
    "no-extra-semi": 1,
    "no-fallthrough": 1,
    "no-floating-decimal": 1,
    "no-func-assign": 1,
    "no-global-assign": 2,
    "no-implicit-coercion": 1,
    "no-implicit-globals": 1,
    "no-implied-eval": 2,
    "no-inline-comments": 0,
    "no-inner-declarations": 1,
    "no-invalid-regexp": 2,
    "no-invalid-this": 2,
    "no-irregular-whitespace": 1,
    "no-iterator": 2,
    "no-label-var": 2,
    "no-labels": 1,
    "no-lone-blocks": 1,
    "no-lonely-if": 1,
    "no-loop-func": 1,
    "no-magic-numbers": 0,
    "no-mixed-operators": 1,
    "no-mixed-requires": 1,
    "no-mixed-spaces-and-tabs": 1,
    "no-multi-spaces": [1, {
      "exceptions": {
        "ArrayExpression": true,
        "ObjectExpression": true
      }
    }],
    "no-multi-str": 1,
    "no-multiple-empty-lines": 1,
    "no-negated-condition": 0,
    "no-nested-ternary": 0,
    "no-new": 1,
    "no-new-func": 1,
    "no-new-object": 1,
    "no-new-require": 0,
    "no-new-symbol": 2,
    "no-new-wrappers": 1,
    "no-obj-calls": 2,
    "no-octal": 1,
    "no-octal-escape": 1,
    "no-param-reassign": 0,
    "no-path-concat": 0,
    "no-plusplus": 0,
    "no-process-env": 0,
    "no-process-exit": 0,
    "no-proto": 1,
    "no-prototype-builtins": 0,
    "no-redeclare": ["error", { "builtinGlobals": true }],
    "no-regex-spaces": 0,
    "no-restricted-globals": 0,
    "no-restricted-imports": 0,
    "no-restricted-modules": 0,
    "no-restricted-properties": 0,
    "no-restricted-syntax": 0,
    "no-return-assign": 2,
    "no-script-url": 1,
    "no-self-assign": 1,
    "no-self-compare": 1,
    "no-sequences": 1,
    "no-shadow": 1,
    "no-shadow-restricted-names": 2,
    "no-sparse-arrays": 1,
    "no-sync": 2,
    "no-tabs": 0,
    "no-template-curly-in-string": 1,
    "no-ternary": 0,
    "no-this-before-super": 2,
    "no-throw-literal": 2,
    "no-trailing-spaces": 0, // handled by editor
    "no-undef": 2,
    "no-undef-init": 1,
    "no-undefined": 1,
    "no-underscore-dangle": 0,
    "no-unexpected-multiline": 2,
    "no-unmodified-loop-condition": 1,
    "no-unneeded-ternary": 1,
    "no-unreachable": 2,
    "no-unsafe-finally": 2,
    "no-unsafe-negation": 2,
    "no-unused-expressions": 1,
    "no-unused-labels": 1,
    "no-unused-vars": 1,
    "no-use-before-define": 2,
    "no-useless-call": 1,
    "no-useless-computed-key": 1,
    "no-useless-concat": 1,
    "no-useless-constructor": 1,
    "no-useless-escape": 1,
    "no-useless-rename": 1,
    "no-var": 1,
    "no-void": 1,
    "no-warning-comments": 1,
    "no-whitespace-before-property": 1,
    "no-with": 2,
    "object-curly-newline": 0,
    "object-curly-spacing": [1, "always"],
    "object-property-newline": 0,
    "object-shorthand": 0,
    "one-var": 0,
    "one-var-declaration-per-line": 0,
    "operator-assignment": 0,
    "operator-linebreak": [1, "after"],
    "padded-blocks": 0,
    "prefer-arrow-callback": 0,
    "prefer-const": 0,
    "prefer-numeric-literals": 1,
    "prefer-reflect": 0,
    "prefer-rest-params": 1,
    "prefer-spread": 1,
    "prefer-template": 1,
    "quote-props": 0,
    "quotes": [1, "double", { "allowTemplateLiterals": true }],
    "radix": 0,
    "require-jsdoc": 0,
    "require-yield": 2,
    "rest-spread-spacing": [1, "never"],
    "semi": [1, "never"],
    "semi-spacing": [1, { "before": false, "after": true }],
    "sort-imports": 0,
    "sort-keys": 0,
    "sort-vars": 0,
    "space-before-blocks": [1, "always"],
    "space-before-function-paren": [1, "never"],
    "space-in-parens": [1, "never"],
    "space-infix-ops": 1,
    "space-unary-ops": [1, { "words": true, "nonwords": false }],
    "spaced-comment": [1, "always"],
    "strict": 0,
    "symbol-description": 0,
    "template-curly-spacing": 0,
    "unicode-bom": 0,
    "use-isnan": 2,
    "valid-jsdoc": 2,
    "valid-typeof": 2,
    "vars-on-top": 0,
    "wrap-iife": 1,
    "wrap-regex": 0,
    "yield-star-spacing": 0,
    "yoda": 1
  }
}
